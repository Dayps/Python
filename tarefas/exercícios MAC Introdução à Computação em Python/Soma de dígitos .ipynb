    "# Soma dos digitos\n",
    "\n",
    "num = int(input('Digite um numero inteiro: '))\n",
    "soma = 0\n",
    "while (num != 0):\n",
    "    resto = num % 10\n",
    "    num = (num - resto) // 10\n",
    "    soma = soma + resto\n",
    "print(f'A soma dos digitos eh: {soma}')"
   ]
  }
